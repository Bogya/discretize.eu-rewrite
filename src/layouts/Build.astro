---
import BuildFrontmatter from "../components/frontmatter/BuildFrontmatter.astro";
import Layout, { SEOProps } from "./Layout.astro";

export interface Props {
  title: string;
}

const { title } = Astro.props.frontmatter;

const seo: SEOProps = {
  title,
};
---

<Layout seo={seo}>
  <BuildFrontmatter frontmatter={Astro.props.frontmatter} />
  <slot />
</Layout>
