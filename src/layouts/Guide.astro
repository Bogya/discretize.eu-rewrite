---
import GuideFrontmatter from "src/components/frontmatter/GuideFrontmatter.astro";
import Layout, { SEOProps } from "./Layout.astro";

export interface Props {
  title: string;
}

const { title } = Astro.props.frontmatter;
const seo: SEOProps = {
  title,
};
---

<Layout seo={seo}>
  <GuideFrontmatter />
  <slot />
</Layout>
